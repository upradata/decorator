{
    "name": "@upradata/decorator",
    "author": {
        "name": "Thomas Milotti",
        "email": "thomas.milotti@gmail.com",
        "url": "https://github.com/milottit"
    },
    "repository": {
        "url": ""
    },
    "license": "MIT",
    "version": "1.0.1",
    "description": "Set of typescript decorators",
    "main": "lib",
    "types": "lib",
    "module": "lib",
    "files": [
        "lib"
    ],
    "scripts": {
        "test": "jest --verbose false --config jest-ut.config.js",
        "e2e": "jest --passWithNoTests --verbose false --config jest-e2e.config.js",
        "clean": "shx rm -rf lib lib-esm",
        "prebuild": "npm run clean",
        "build": "tsc -p ./tsconfig.src.json",
        "postbuild": "npm run test && npm run e2e",
        "build:only": "npm run prebuild && tsc && tsc -m es6 --outDir lib-esm",
        "build:watch": "npm run tsc -w",
        "github-push": "npm run build && npm version patch && git push && git push --tags",
        "npm-publish": "npm run build && npm publish --access public"
    },
    "dependencies": {
        "@angular/core": "^8.2.14",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@types/node": "^13.1.6",
        "fixed-width-string": "^1.1.0",
        "jest": "^24.9.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.4",
        "shx": "^0.3.2",
        "ts-jest": "^24.3.0",
        "ts-node": "^8.5.4",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.1",
        "typescript": "^3.7.4"
    },
    "keywords": [
        "typescript",
        "decorators"
    ],
    "publishConfig": {
        "access": "public"
    },
    "local": {
        "dependencies": {
            "@upradata/node-util": "link:/home/milottit/Libraries/Util/node-util@2.0.3",
            "@upradata/util": "link:/home/milottit/Libraries/Util/util@1.0.24"
        }
    }
}
